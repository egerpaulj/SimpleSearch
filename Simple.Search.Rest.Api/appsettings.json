{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SearchOptions": {
    "FolderBasedSearchConfigurations": [
      {
        "SearchType": 2,
        "Uri": "wwwroot",
        "Title": "Local pages",
        "XpathToTitle": "//h2[contains(@class, 'content-title')]",
        "SearchPattern": "*.html"
      }
    ]
  }
}
